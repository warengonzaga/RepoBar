{
  "schemaNamespace": "RepoBarGraphQL",
  "schemaDownloadConfiguration": {
    "downloadMethod": {
      "introspection": {
        "endpointURL": "https://api.github.com/graphql",
        "httpMethod": {
          "POST": {}
        },
        "includeDeprecatedInputValues": false,
        "outputFormat": "SDL"
      }
    },
    "headers": {
      "Authorization": "Bearer ${GITHUB_TOKEN}",
      "User-Agent": "RepoBar-Codegen"
    },
    "outputPath": "GraphQL/schema.graphqls"
  },
  "input": {
    "schemaSearchPaths": [
      "GraphQL/schema.graphqls"
    ],
    "operationSearchPaths": [
      "GraphQL/**/*.graphql"
    ]
  },
  "output": {
    "schemaTypes": {
      "path": "Sources/RepoBar/API/Generated/SchemaTypes.swift",
      "moduleType": {
        "embeddedInTarget": {
          "name": "RepoBar",
          "accessModifier": "internal"
        }
      }
    },
    "operations": {
      "inSchemaModule": {
        "moduleType": {
          "embeddedInTarget": {
            "name": "RepoBar",
            "accessModifier": "internal"
          }
        },
        "operationIDsPath": "GraphQL/operation_ids.json"
      }
    },
    "testMocks": {
      "none": {}
    }
  },
  "experimentalFeatures": {
    "clientControlledNullability": true
  }
}
